{{input type="text" name="email" value=model.new_team.email class="email" placeholder="email"}}
{{#paper-radio-group class="phone-list"
  groupValue=(readonly currentPhone)
  onChange=(action (mut currentPhone)) as |group|}}
  {{#each model.phone_list as |phoneNumber|}}
    {{#group.radio value=phoneNumber.value}} {{phoneNumber.value}} {{/group.radio}}
  {{/each}}
{{/paper-radio-group}}

{{#paper-button onClick=(action "create")}}Create{{/paper-button}}
